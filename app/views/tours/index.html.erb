<h2>All tours</h2>
<% @tour.each do |tour| %>
  <h4><%= tour.title %></h4>
  <h5><%= tour.location %></h5>
  <p><%= tour.description %> | <%= tour.price %></p>
  <p><%= tour.user_id %> | <%= tour.user %></p>

  create_table "tours", force: :cascade do |t|
    t.string "title"
    t.string "location"
    t.text "description"
    t.integer "price"
    t.bigint "user_id"
    t.date "date"
    t.integer "capacity"
    t.datetime "created_at", null: false
    t.datetime "updated_at", null: false
    t.string "photo"
    t.index ["user_id"], name: "index_tours_on_user_id"
  end
